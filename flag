#include <stdio.h>
void main()
{
    int i,n,flag;
    scanf("%d",&n);
    i=1;
    flag=0;
    while(i<=n)
    {
        if(flag==0){
            printf("%d",i);
            flag=1;
        }
        else
        {
            printf(",%d ",i);
            }
            i++;
        }
}
output:
5
1,2 ,3 ,4 ,5
    
