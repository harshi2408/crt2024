#include <stdio.h>
void main()
{
    int i,n,flag;
    scanf("%d",&n);
    i=n;
    flag=0;
    while(i>=1)
    {
        if(flag==0){
            printf("%d",i);
            flag=1;
        }
        else
        {
            printf(",%d ",i);
            }
            i--;
        }
}
    output:
6
6,5 ,4 ,3 ,2 ,1 

